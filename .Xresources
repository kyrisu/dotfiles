! urxvt
URxvt*buffered:           true
URxvt*cursorBlink:        true
URxvt*underlineColor:     yellow
!URxvt*font:               xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true
URxvt*font:               xft:Monaco\ for\ Powerline:pixelsize=13:antialias=true
URxvt*depth:              32
URxvt*borderless:         1
URxvt*scrollBar:          false
URxvt*loginShell:         true
Urxvt*secondaryScroll:    true    # Enable Shift-PageUp/Down in screen
URxvt*saveLines:          5000
URxvt*termName:           rxvt-unicode
!URxvt*transparent:        true
URxvt*shading:            10
URxvt*clipboard.autocopy: true
URxvt.perl-ext-common:    default,clipboard,matcher,font-size
URxvt.url-launcher:       /usr/bin/google-chrome-beta
URxvt.matcher.button:     1

URxvt.keysym.C-equal:     perl:font-size:increase
URxvt.keysym.C-minus:     perl:font-size:decrease
URxvt.keysym.C-0:         perl:font-size:reset
URxvt.keysym.Shift-Insert: perl:clipboard:paste

URxvt.cursorColor: #06989a
!URxvt.background: #000000  
URxvt.background: rgba:0000/0000/0200/c800
URxvt.foreground: #cccccc  
URxvt.color0:  #000000  
URxvt.color1:  #cc0000  
URxvt.color2:  #4E9A06  
URxvt.color3:  #C4A000  
URxvt.color4:  #3465A4  
URxvt.color5:  #75507B  
URxvt.color6:  #06989A  
URxvt.color7:  #D3D7CF  
URxvt.color8:  #555753  
URxvt.color9:  #EF2929  
URxvt.color10: #8AE234  
URxvt.color11: #FCE94F  
URxvt.color12: #739FCF  
URxvt.color13: #AD7FA8  
URxvt.color14: #34E2E2  
URxvt.color15: #EEEEEC  
